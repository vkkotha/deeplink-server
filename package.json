{
  "name": "deeplink-server",
  "version": "1.0.0",
  "description": "deeplink server",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "postinstall": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "jquery": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "vijay kotha",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "express": "4.14.0",
    "express-es6-template-engine": "1.0.9",
    "jquery-lite": "2.2.3",
    "resource-router-middleware": "0.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-watch": "2.0.5",
    "eslint": "3.13.1",
    "nodemon": "1.11.0"
  }
}
